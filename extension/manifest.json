{
    "manifest_version": 3,
    "name": "Activity Tracker",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    }
    
  }